{
  "name": "@techmely/head",
  "description": "Head manager SSR",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "homepage": "https://techmely.com/libraries/head",
  "bugs": {
    "url": "https://github.com/techmely/head/issues"
  },
  "repository": {
    "url": "git+https://github.com/techmely/head.git#main"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test vitest run --passWithNoTests --isolate",
    "build": "bun build ./src/index.ts --target bun --outdir dist --sourcemap=linked && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.7",
    "@techmely/types": "1.9.0",
    "@types/bun": "1.1.6",
    "@vitest/coverage-v8": "2.0.2",
    "bun": "1.1.18",
    "typescript": "5.5.3",
    "vitest": "2.0.2"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "tag": "latest"
  },
  "funding": [
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/techmely"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/techmely"
    }
  ],
  "keywords": [
    "techmely",
    "head manager"
  ]
}
